<div class="nested-fields">
  
  <div class="form-group row">
    <%= f.label :name, class: "col-sm-2 col-form-label" %>
    <div class="col-sm-10">
      <%= f.text_field :name, class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :email, class: "col-sm-2 col-form-label" %>
    <div class="col-sm-10">
      <%= f.text_field :email, class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :telephone, class: "col-sm-2 col-form-label" %>
    <div class="col-sm-10">
      <%= f.text_field :telephone, class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :cell_phone, class: "col-sm-2 col-form-label" %>
    <div class="col-sm-10">
      <%= f.text_field :cell_phone, class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :contact_type_id, class: "col-sm-2 col-form-label" %>
    <div class="col-sm-10">
      <%= f.select("contact_type_id", @contact_type_options, { include_blank: 'Selecione...'}, class: "form-control") %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :excluded, class: "col-sm-2 col-form-label" %>
    <div class="col-sm-10">
    <%= f.check_box :excluded, class: "checkbox" %>
    </div>
  </div>

  <%= link_to_remove_association('[remove]', f) %>
  <br></br>
</div>


  
